{
  "name": "kengine",
  "version": "0.0.2",
  "description": "A GameMaker framework that helps with modding functions for your projects.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knno/kengine-archive.git"
  },
  "keywords": [
    "gamemaker",
    "GML",
    "engine",
    "game"
  ],
  "author": "Kenan Masri <kenanmasri@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knno/kengine-archive/issues"
  },
  "homepage": "https://github.com/knno/kengine-archive#readme",
  "scripts": {
    "docs:api": "node docs/scripts/jsdoc2md2.js --configure docs/jsdoc.json ./**/*.gml --partials docs/templates/partials",
    "docs:serve": "python -m http.server -d docs",
    "docs:api-md": "npx jsdoc2md --configure docs/jsdoc.json ./**/*.gml --partial docs/templates/partials/*.hbs > docs.md"
  },
  "devDependencies": {
    "docsify": "^4.11.4",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.1",
    "minimist": "^1.2.8"
  }
}
